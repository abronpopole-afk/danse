modules = ["web", "python-3.11", "postgresql-16", "nodejs-20"]
run = "npm run dev"
hidden = [".config", ".git", "generated-icon.png", "node_modules", "dist"]

[nix]
channel = "stable-24_05"
packages = ["gcc-unwrapped", "gnumake", "imagemagick", "libpng", "openssh", "openssl", "pkg-config", "python311", "python311Full", "python311Packages.fastapi", "python311Packages.numpy", "python311Packages.opencv4", "python311Packages.pip", "python311Packages.python-multipart", "python311Packages.uvicorn", "wget", "xorg.libX11", "xorg.libXext", "xorg.libXi", "xorg.libXinerama", "xorg.libXtst", "zlib"]

[deployment]
deploymentTarget = "autoscale"
build = ["npm", "run", "build"]
publicDir = "dist/public"
run = ["npm", "run", "start"]

[[ports]]
localPort = 5000
externalPort = 80

[env]
PORT = "5000"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Start application"

[[workflows.workflow]]
name = "Start application"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"
waitForPort = 5000

[agent]
mockupState = "FULLSTACK"
integrations = ["javascript_database:1.0.0"]
